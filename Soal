#Fungsi Kuadrat
3. F(x) = x^2 - 2x - 3

#Fungsi Polinomial
4. F(x) = ax^3 + bx^2 + cx + d

#Fungsi Rasional
5. F(x) = x / x - 1


#Fungsi Konstan: F(x) = 4
f <- function(x){
  fx <- 4
  return (fx)
}
input <- 0:10
plot ( input, sapply (input, f), type = "l", xlab = "x", ylab = "f(x)" )


#Fungsi Linear: F(x) = 5x + 8
f <- function(x){
  fx <- 5*x + 8
  return(fx)
}
input <- -1:10
plot (input, sapply(input, f), type = "l", xlab = "x", ylab = "f(x)")

